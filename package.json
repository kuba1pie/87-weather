{
  "name": "vuetemplate",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.12",
    "chart.js": "^3.9.1",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "pinia": "^2.0.22",
    "prettier": "^2.7.1",
    "unplugin-auto-import": "^0.10.3",
    "vue": "^3.2.39",
    "vue-chart-3": "^3.1.8",
    "vue-echarts": "^6.2.3",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.55.0",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.3",
    "unocss": "^0.45.24",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-pages": "^0.25.0",
    "vitest": "^0.20.3",
    "vue-tsc": "^0.39.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{*}": [
      "eslint --fix"
    ]
  }
}
